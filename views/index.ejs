<div class="links">
  <a href="/posts/create">Add Posts</a>
</div>
<main>
    <div class="articles__list">
      <% posts.forEach(function(post) { %>
        <div class="">
          <a href="/posts/<%= post.id %>">
            <img src="<%= post.image_loc %>" alt="">
            <p><%- post.desc %></p>
          </a>
        </div>
      <% }) %>
    </div>
</main>
